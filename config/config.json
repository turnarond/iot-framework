{
  "server": {
    "ip": "127.0.0.1",
    "port": 8080,
    "name": "VideoServer",
    "version": "1.0.0"
  },
  "database": {
    "path": "/home/yanchaodong/work/acoinfo/edge-framework/data/bas-business.db",
    "maxConnections": 10,
    "timeout": 30
  },
  "zlmediakit": {
    "serverIp": "127.0.0.1",
    "serverPort": 1935,
    "appName": "live",
    "streamPrefix": "camera_",
    "enableRtsp": true,
    "enableRtmp": true,
    "enableHls": true,
    "enableFlv": true,
    "enableWebRtc": true
  },
  "ffmpeg": {
    "path": "/usr/bin/ffmpeg",
    "timeout": 30,
    "logLevel": "error"
  },
  "stream": {
    "defaultWidth": 1280,
    "defaultHeight": 720,
    "defaultBitrate": 2000,
    "defaultFps": 25,
    "maxStreams": 100
  },
  "webserver": {
    "url": "http://localhost:3000",
    "timeout": 10,
    "retryInterval": 5
  },
  "notification": {
    "maxRetries": 3,
    "retryInterval": 2000,
    "queueSize": 100,
    "enableAsync": true
  },
  "logging": {
    "level": "info",
    "path": "/home/yanchaodong/work/acoinfo/edge-framework/logs",
    "maxFileSize": 10485760,
    "maxFiles": 5
  },
  "plugins": {
    "camera": {
      "path": "./plugins/camera",
      "enabled": true
    },
    "stream": {
      "path": "./plugins/stream",
      "enabled": true
    }
  }
}